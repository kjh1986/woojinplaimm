<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- <mapper></mapper> 엘리먼트는 MyBatis 설정 XML 파일의 루트 엘리먼트 -->  
<!-- namespcae="" 메소드 호출시 식별자로 사용된다. -->
<mapper namespace="com.woojinplaimm.edu.WoojinMapper">

	<select id="login" resultType="Login">
		SELECT admin_id 
		FROM admin_
		WHERE admin_id=#{admin_id} AND admin_pass=#{admin_pass}
	</select>
	
</mapper>  